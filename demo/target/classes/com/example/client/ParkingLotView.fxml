<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.client.ParkingLotController" spacing="20" alignment="CENTER" styleClass="vbox" prefWidth="900" prefHeight="600">
    <Label fx:id="parkingLotLabel" text="Parking Lot" styleClass="title-label"/>
    <ImageView fx:id="parkingLotImageView" fitWidth="400" fitHeight="300" styleClass="image-view"/>
    <CheckBox text="I am parked here" fx:id="parkedHereCheckBox" styleClass="custom-checkbox"/>
    <Button fx:id="backButton" text="Back" styleClass="button"/>
</VBox>
